<resources>

    <!-- Base application theme. -->
    <style name="Original" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Tropical" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary_tropical</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_tropical</item>
        <item name="colorAccent">@color/colorAccent_tropical</item>
    </style>

    <style name="db" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary_db</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_db</item>
        <item name="colorAccent">@color/colorAccent_db</item>
    </style>

    <style name="muted" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary_muted</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_muted</item>
        <item name="colorAccent">@color/colorAccent_muted</item>
    </style>


</resources>
